<%= form_for @product ,url: {action: 'update'} do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :image %>
    <%= f.text_field :image %> <br>
    <%= f.label :quantity %>
    <%= f.text_field :quantity %> <br>
    <%= f.label :price %>
    <%= f.text_field :price %> <br>
    <%= f.submit %>


<% end %>